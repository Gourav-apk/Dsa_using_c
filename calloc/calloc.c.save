#include<stdio.h>
#include<stdlib.h>
int main()
{
    int n;
    printf("Enter the character length\n");
    scanf("%d",&n);
    char *p=(char *)calloc(n,sizeof(char));
    printf("Enter the %d character \n",n);
    for(int i=0;i<n;i++)
    {
        scanf("%c",&p[i]);
    }
     for(int j=0;j<n;j++)
    {
        printf("Character is %c ",p[j]);
    }
    return 0;
}
