#include<stdio.h>
#include<stdlib.h>
int main()
{
    int n;
    printf("Enter the array length length\n");
    scanf("%d",&n);
    int *p=(int *)calloc((n),sizeof(int));
    printf("Enter the %d integer values \n",n);
   
     for(int j=1;j<=n;j++)
    {
        printf("Integer is %d \n ",p[j]);
    }
    return 0;
}
