#include<stdio.h>
void selectionSort(int arr[],int n)
{
    int i,j,minIndex ,temp;
    for(i=0;i<n-1;i++)
    {
        minIndex=i;
        for(j=i+1;j<n;j++)
        {
            if(arr[j]<arr[minIndex])
                minIndex=j;
        }
        temp=arr[minIdex];
        arr[minIndex] =arr[i];
        arr[i]=temp;
    }
}
void printArray(int arr[],int n)
{
    int i;
    for(i=0;i<n;i++)
    {
         printf("%d ",arr[i]);
    }
    printf("\n");

}
int main()
{
    int arr[100],n,i;
    printf("Enter number of element \n");
    scanf("%d",&n);
